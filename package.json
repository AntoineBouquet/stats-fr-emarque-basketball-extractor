{
  "name": "stats-fr-emarque-basketball-extractor",
  "version": "1.0.2",
  "description": "A tool to extract french basketball amateur matches stats from E-Marque (FFBB software)",
  "main": "index.js",
  "scripts": {
    "test": "node $NODE_DEBUG_OPTION ./tests/check-file.js && node $NODE_DEBUG_OPTION ./tests/extractor.js",
    "test:slow": "node $NODE_DEBUG_OPTION --max-old-space-size=150 ./tests/check-file.js && node $NODE_DEBUG_OPTION --max-old-space-size=150 ./tests/extractor.js",
    "check-file": "node $NODE_DEBUG_OPTION ./tests/check-file.js",
    "extract-data": "node $NODE_DEBUG_OPTION ./tests/extract-data.js"
  },
  "author": "Antoine Bouquet",
  "license": "Apache-2.0",
  "dependencies": {
    "assert": "^2.0.0",
    "async-lock": "^1.2.2",
    "node-ts-ocr": "^1.0.15",
    "pdf-image": "^2.0.0",
    "pdf.js-extract": "^0.1.3",
    "pixelmatch": "^5.1.0",
    "pngjs": "^3.4.0",
    "potrace": "^2.1.2",
    "uuid": "^3.3.3"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "emarque",
    "basketball",
    "extractor",
    "stats"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AntoineBouquet/stats-fr-emarque-basketball-extractor.git"
  }
}
